{
  "build": {
    "beforeDevCommand": "cd ./src && npm run dev",
    "beforeBuildCommand": "cd ./src && npm run build",
    "devPath": "http://localhost:5173",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Meeting Baas",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },

      "icon": ["icons/icon.icns"],
      "externalBin": [],
      "identifier": "com.Spoke.app",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Meeting Baas",
        "width": 800
      }
    ]
  }
}
